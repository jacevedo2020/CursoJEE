	<queryString>
		<![CDATA[SELECT 
    factura.id,
    factura.fecha,
	factura.numero,
	cliente.cedula,
	cliente.nombres,
	cliente.apellidos,
	(SELECT SUM( d.cantidad * d.precio)
FROM detalle_factura d
WHERE 
	 d.id_factura = factura.id ) subtotal
FROM cliente
	INNER JOIN factura ON 
	 factura.id_cliente = cliente.id]]>
	</queryString>
